<fieldset>
  <table>
  <tr>
    <td>
      <%= f.select :genre_id, options_from_collection_for_select(Genre.all, 'id', 'name', f.object.genre_id) %>
    </td>
    <td>
      <%= f.select :looking_for_band, [['Yes', true], ['No', false]]  %>
    </td>
    <td>
      <%= f.hidden_field :_destroy %>
      <%= link_to image_tag("delete.png", size:'16x16'), "#", class: "remove_fields" %>
    </td>
  </tr>
</table>
</fieldset>
