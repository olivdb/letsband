<% provide(:title, "Sign in") %>

<center>
<h1>Sign in</h1>
<div class="well table_signup text2 center">

    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>
<br/>
       <%= f.submit "Sign in !", style:"width:97%;",  class: "btn btn-medium btn-success" %>

    <% end %>

    <p>New user? <%= link_to "Sign up now!", signup_path %></p>
  </div>

</center>
<br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />